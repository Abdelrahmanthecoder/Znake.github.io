{
  "name": "Znake-alpha",
  "version": "0.1.3",
  "description": "The new snake game in town!",
  "manifest_version": 3,
  "background": {
    "service_worker": "snake.js"
  },
  "action": {
    "default_title": "Znake-alpha",
    "default_popup": "index2.html"
  },
  "host_permissions": [
    "<all_urls>"
  ],
  "permissions": [
    "webRequest",
    "webRequestBlocking"
  ]
}
